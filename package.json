    "release:patch": "git checkout master && npm version patch -m \"chore(update): patch release %s ğŸ› \"",
    "release:minor": "git checkout master && npm version minor -m \"chore(update): release %s ğŸš€\"",
    "release:major": "git checkout master && npm version major -m \"chore(update): major release %s ğŸ’¥ \""
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COEXCZ/geosearch.git"
  },
  "keywords": [
    "mapycz",
    "maps",
    "mapy",
    "geocoding"
  ],
  "author": "COEX (https://github.com/COEXCZ)",
