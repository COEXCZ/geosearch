{
  "name": "@coex/geosearch",
  "version": "0.6.2",
  "description": "Nodejs library for finding places using Mapy.cz Suggest API",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "npm-prepare-dist",
    "demo": "ts-node demo",
    "gpr:setup": "ts-node scripts/gpr-setup",
    "test": "jest --config jestconfig.json",
    "lint": "eslint ./src/**/**/* --fix",
    "postversion": "git push && git push --follow-tags",
    "publish:next": "yarn && yarn build && yarn test --coverage true && npm publish --folder dist --tag beta",
    "release:beta": "npm version prerelease -m \"chore(update): prelease %s Î²\"",
    "release:patch": "git checkout master && npm version patch -m \"chore(update): patch release %s ğŸ› \"",
    "release:minor": "git checkout master && npm version minor -m \"chore(update): release %s ğŸš€\"",
    "release:major": "git checkout master && npm version major -m \"chore(update): major release %s ğŸ’¥ \""
  },
  "dependencies": {
    "axios": "^0.26.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COEXCZ/geosearch.git"
  },
  "keywords": [
    "mapycz",
    "maps",
    "mapy",
    "geocoding"
  ],
  "author": "COEX (https://github.com/COEXCZ)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/COEXCZ/geosearch/issues"
  },
  "homepage": "https://github.com/COEXCZ/geosearch#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "npm-prepare-dist": "^0.4.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn run lint"
    }
  }
